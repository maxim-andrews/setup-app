{
  "private": true,
  "scripts": {
    "start": "fullstack-scripts start",
    "build": "fullstack-scripts build",
    "test": "fullstack-scripts test --env=jsdom"
  },
  "devDependencies": {
    "connected-react-router": "^6.5.2",
    "fullstack-scripts": "2.0.0-alpha.23",
    "history": "^4.10.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "node-sass": "^4.12.0",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "^15.7.2",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^8.0.0"
  },
  "dependencies": {
    "koa": "^2.10.0",
    "koa-compress": "^3.0.0",
    "koa-morgan": "^1.0.1",
    "koa-range": "^0.3.0",
    "koa-rewrite": "^3.0.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "setup-app-ssr": "2.0.0-alpha.23"
  },
  "eslintConfig": {
    "extends": "setup-app"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 9"
    ]
  },
  "workboxSW": {
    "importWorkboxFrom": "cdn",
    "navigateFallback": "/index.html",
    "navigateFallbackBlacklist" : [
      {
        "regexp": "^/_"
      },
      {
        "regexp": "/[^/?]+\\.[^/]+$"
      }
    ],
    "exclude": [
      {
        "regexp": "\\.map$"
      },
      {
        "regexp": "manifest\\.json$"
      }
    ]
  },
  "setupApp": {
    "defaultIndex": "index.html",
    "csr": {
      "buildPath": "build/client",
      "devRewrite": {
        "regexp": "^\\/(spin|blink)$"
      }
    },
    "ssr": {
      "methods": {
        "main": "src/index.ssr.js",
        "initStore": "src/Store/initStore.js",
        "configureStore": "src/Store/ConfigureStore.js"
      },
      "ssrMiddleware": "server/ssrMiddleware.js",
      "buildPath": "build/server"
    },
    "backendAfter": "server/routes.js",
    "watchBackendFiles": "src/**/*.backend.js"
  }
}
