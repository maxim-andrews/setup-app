{
  "private": true,
  "description": "This project was created by `setup-app`",
  "scripts": {
    "start": "fullstack-scripts start",
    "build": "fullstack-scripts build",
    "test": "fullstack-scripts test --env=jsdom"
  },
  "devDependencies": {
    "fullstack-scripts": "^2.0.0-alpha.6",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "koa": "^2.7.0",
    "koa-compress": "^3.0.0",
    "koa-morgan": "^1.0.1",
    "koa-range": "^0.3.0",
    "koa-static": "^5.0.0",
    "setup-app-ssr": "^2.0.0-alpha.6"
  },
  "eslintConfig": {
    "extends": "setup-app"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 9"
    ]
  },
  "serviceWorkerPreCache": {
    "navigateFallback": "/index.html",
    "navigateFallbackWhitelist": [
      {
        "regexp": "^(?!\\/__).*"
      }
    ],
    "staticFileGlobsIgnorePatterns": [
      {
        "regexp": "\\.map$"
      },
      {
        "regexp": "manifest\\.json$"
      },
      {
        "regexp": "asset-manifest\\.json$"
      }
    ]
  },
  "defaultIndex": "index.html",
  "frontEndRendering": {
    "buildPath": "build/client"
  },
  "serverSideRendering": {
    "methods": {
      "main": "src/server.side.renderer.js"
    },
    "ssrMiddleware": "server/ssrMiddleware.js",
    "buildPath": "build/server"
  },
  "appBackend": "server/routes.js",
  "setup-app": {
    "label": "React Only",
    "styles": [
      {
        "label": "CSS Only",
        "patterns": "*\\/css\\/([^\\/]+\\.css)"
      },
      {
        "label": "Post CSS Only",
        "pattern": "*\\/postcss\\/([^\\/]+\\.css)",
        "modules": [
          "postcss-preset-env"
        ]
      },
      {
        "label": "SASS Only",
        "pattern": "*\\/sass\\/([^\\/]+\\.scss)",
        "modules": [
          "node-sass",
          "sass-loader"
        ]
      },
      {
        "label": "LESS Only",
        "pattern": "*\\/less\\/([^\\/]+\\.less)",
        "modules": [
          "less",
          "less-loader"
        ]
      },
      {
        "label": "Post CSS & SASS",
        "pattern": "*\\/postcss-sass\\/([^\\/]+\\.s?css)",
        "modules": [
          "postcss-preset-env",
          "node-sass",
          "sass-loader"
        ]
      },
      {
        "label": "Post CSS & LESS",
        "pattern": "*\\/postcss-less\\/([^\\/]+\\.(le|c)ss)",
        "modules": [
          "postcss-preset-env",
          "less",
          "less-loader"
        ]
      },
      {
        "label": "Post CSS & SASS & LESS",
        "pattern": "*\\/postcss-sass-less\\/([^\\/]+\\.(le|s?c)ss)",
        "modules": [
          "postcss-preset-env",
          "node-sass",
          "sass-loader",
          "less",
          "less-loader"
        ]
      },
      {
        "label": "SASS & LESS",
        "pattern": "*\\/sass-less\\/([^\\/]+\\.(le|s?c)ss)",
        "modules": [
          "node-sass",
          "sass-loader",
          "less",
          "less-loader"
        ]
      }
    ],
    "server": [
      {
        "block": {
          "start": "server-side-rendering-start",
          "end": "server-side-rendering-end"
        },
        "files": [
          "server/routes.js",
          "server/ssrMiddleware.js"
        ],
        "pkgJsn": {
          "dependencies": [
            "setup-app-ssr"
          ],
          "defaultIndex": "index.html",
          "frontEndRendering": {
            "buildPath": "build/client"
          },
          "serverSideRendering": {
            "methods": {
              "main": "src/server.side.renderer.js"
            },
            "ssrMiddleware": "server/ssrMiddleware.js",
            "buildPath": "build/server"
          },
          "appBackend": "server/routes.js"
        }
      },
      {}
    ]
  }
}
