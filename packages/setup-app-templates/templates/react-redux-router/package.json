{
  "private": true,
  "description": "This project was created by `setup-app`",
  "scripts": {
    "start": "fullstack-scripts start",
    "build": "fullstack-scripts build",
    "test": "fullstack-scripts test --env=jsdom"
  },
  "devDependencies": {
    "connected-react-router": "^6.1.0",
    "fullstack-scripts": "^1.3.8",
    "history": "^4.7.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1"
  },
  "dependencies": {
    "koa": "^2.6.2",
    "koa-compress": "^3.0.0",
    "koa-morgan": "^1.0.1",
    "koa-range": "^0.3.0",
    "koa-static": "^4.0.3"
  },
  "eslintConfig": {
    "extends": "setup-app"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 9"
    ]
  },
  "serviceWorkerPreCache": {
    "navigateFallback": "/index.html",
    "navigateFallbackWhitelist": [
      {
        "regexp": "^(?!\\/__).*"
      }
    ],
    "staticFileGlobsIgnorePatterns": [
      {
        "regexp": "\\.map$"
      },
      {
        "regexp": "manifest\\.json$"
      },
      {
        "regexp": "asset-manifest\\.json$"
      }
    ]
  },
  "setup-app": [
    "react",
    "redux",
    "router"
  ]
}
