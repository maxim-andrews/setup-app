  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxim-andrews/webpack-koa-server/issues"
  },
  "homepage": "https://github.com/maxim-andrews/webpack-koa-server#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.7.0",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.6.0",
    "webpack": "^4.22.0"
  },
  "dependencies": {
    "address": "^1.0.3",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
